{
  "name": "elvys",
  "version": "1.0.0",
  "description": "A minimalist framework for rapid UI development",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "sass sass/src/_index.scss:dist/1.0.0/css/ui.css --no-source-map && sass sass/src/_index.scss:dist/1.0.0/css/ui.min.css --no-source-map --style=compressed",
    "sass-dev": "sass sass/src:sass/built --watch",
    "tsc": "npx tsc && esbuild dist/1.0.0/js/ui.js --minify --outfile=dist/1.0.0/js/ui.min.js",
    "tsc-dev": "npx tsc -w"
  },
  "author": "Beba Godfried <me@bebagodfried.com>",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.16.10",
    "sass": "^1.54.9",
    "typescript": "^4.8.3"
  }
}
